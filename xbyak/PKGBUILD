pkgname=xbyak
pkgver=4.30
_commit=702d6e6683c322f08a36ea059f6d6f8263b1bd0d
pkgrel=1
pkgdesc='x86, x64 JIT assembler'
arch=('any')
url='http://homepage1.nifty.com/herumi/soft/xbyak_e.html'
depends=()
license=('BSD')
source=("$pkgname-$pkgver.tar.gz::https://github.com/herumi/xbyak/archive/$_commit.tar.gz")

build() {
  :
}

package() {
  cd "$srcdir/$pkgname-$_commit"
  make PREFIX="$pkgdir/usr" install
  install -Dm644 COPYRIGHT "$pkgdir/usr/share/licenses/$pkgname/COPYRIGHT"
}

